<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ovh.dessert.tpe.repertoiredestagesm2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MenuActivity" android:label="@string/title_activity_menu" />
        <activity android:name=".DebuggingActivity" android:label="@string/title_activity_debug"/>
        <activity android:name=".SearchMap" android:label="@string/title_activity_search_map" />
        <activity android:name=".EntreListeActivity" android:label="@string/title_activity_entreliste" />
        <activity android:name=".MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data android:name="com.google.android.geo.API_KEY" android:value="@string/google_maps_key"/>
    </application>

</manifest>